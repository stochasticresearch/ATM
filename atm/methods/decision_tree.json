        "min_samples_leaf": {
            "type": "int",
            "range": [1, 3]
        }
    },
    "root_hyperparameters": ["criterion", "max_features", "max_depth", "min_samples_split", "min_samples_leaf"],
    "conditional_hyperparameters": {}
}
